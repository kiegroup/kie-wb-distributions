  <stylesheet src='css/guvnor-showcase.css'/>
  <stylesheet src='css/uberfire-showcase.css'/>
  <stylesheet src='css/chome-override.css'/>
  <stylesheet src='css/console.css'/>

  <inherits name="com.google.gwt.xml.XML" />

  <!-- Specify the paths for translatable code -->
  <source path='client'/>
    
